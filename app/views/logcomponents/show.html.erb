<%- model_class = Logcomponent -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:component_id) %>:</strong></dt>
  <dd><%= @logcomponent.component_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:send_date) %>:</strong></dt>
  <dd><%= @logcomponent.send_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sent_from) %>:</strong></dt>
  <dd><%= @logcomponent.sent_from %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sent_by) %>:</strong></dt>
  <dd><%= @logcomponent.sent_by %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sent_to) %>:</strong></dt>
  <dd><%= @logcomponent.sent_to %></dd>
  <dt><strong><%= model_class.human_attribute_name(:on_unit) %>:</strong></dt>
  <dd><%= @logcomponent.on_unit %></dd>
  <dt><strong><%= model_class.human_attribute_name(:arrive_date) %>:</strong></dt>
  <dd><%= @logcomponent.arrive_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:recived_by) %>:</strong></dt>
  <dd><%= @logcomponent.recived_by %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @logcomponent.status %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              logcomponents_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_logcomponent_path(@logcomponent), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              logcomponent_path(@logcomponent),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
