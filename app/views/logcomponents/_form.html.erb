<%= form_for @logcomponent, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :component_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :component_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :send_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :send_date, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :sent_from, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :sent_from, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :sent_by, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :sent_by, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :sent_to, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :sent_to, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :on_unit, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :on_unit, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :arrive_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :arrive_date, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :recived_by, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :recived_by, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :status, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :status, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                logcomponents_path, :class => 'btn' %>
  </div>
<% end %>
